<div class="text-center">
<h1>Lista de Usuarios</h1>
</div>
<div class="row"> 
  <div class="col-md-5 center-block no-float">

<table class="table table-bordered table-striped"> 
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Cedula</th>
      <th>Email</th>
      <th colspan="3" class="text-center">Opciones</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.Nombre %></td>
        <td><%= user.Apellido %></td>
        <td><%= user.Cedula %></td>
        <td><%= user.Email %></td>
        <td><%= link_to 'Ver', user %></td>
        <td><%= link_to 'Editar', edit_user_path(user) %></td>
        <td><%= link_to 'Borrar', user, method: :delete, data: { confirm: 'Confirmar' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<div class="text-center">
<%= link_to 'Nuevo Usuario', new_user_path %>
</div>
</div></div>